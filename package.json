{
  "name": "papertrends-frontend",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@8.15.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next out node_modules/.cache",
    "analyze": "ANALYZE=true next build",
    "preview": "npx serve@latest out",
    "deploy": "wrangler pages deploy out",
    "pages:dev": "wrangler pages dev out"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "d3": "^7.8.5",
    "next": "^15",
    "react": "^18",
    "react-dom": "^18",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@eslint/js": "^9",
    "@types/d3": "^7.4.3",
    "@types/node": "^20",
    "@types/react": "^18",
    "@typescript-eslint/eslint-plugin": "^8",
    "@typescript-eslint/parser": "^8",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "^15",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.0",
    "typescript": "^5"
  }
}